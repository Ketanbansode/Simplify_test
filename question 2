n=int(input("Enter test cases how much want to do"))
arr=[]
for j in range (0,n):
    x=int(input("Total no. of players between Gihun And Ali"))
    y=int(input("Enter the height of both them"))
    count=0
    for i in range (x):
        arr.append(int(input("enter the height of each person")))
        if (arr[i]>y):
            count+=1
    print("total no. of players who will be shot:",count)
